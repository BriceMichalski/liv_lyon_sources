{% trans_default_domain 'FOSUserBundle' %}
{% extends "LIVUserBundle::layout.html.twig" %}

{% block fos_user_content %}
    <div class="row">
        <div class="col s12 m5">
            <div class="card-panel fos_user_user_show">
                <p class="white-text">{{ 'profile.show.username'|trans }}: {{ user.username }}</p>
                <p class="white-text">{{ 'profile.show.email'|trans }}: {{ user.email }}</p>
            </div>
        </div>
    </div>
{% endblock %}
